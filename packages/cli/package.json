{
  "name": "dmnotes",
  "description": "Run a server that hosts your DM notes",
  "version": "0.1.0",
  "author": "John Kane @kanej",
  "bin": {
    "dmnotes": "./bin/run"
  },
  "bugs": "https://github.com/kanej/dmnotes/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "apollo-server-express": "2.19.0",
    "chalk": "4.1.0",
    "cli-ux": "5.5.1",
    "cors": "2.8.5",
    "express": "4.17.1",
    "handlebars": "4.7.7",
    "js-yaml": "3.14.1",
    "rehype-format": "3.1.0",
    "rehype-stringify": "8.0.0",
    "remark-frontmatter": "3.0.0",
    "remark-parse": "9.0.0",
    "remark-rehype": "8.0.0",
    "remark-stringify": "9.0.1",
    "tslib": "^1",
    "unified": "9.2.1",
    "unist-util-find": "1.0.2",
    "unist-util-is": "4.0.4",
    "unist-util-remove": "2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@types/express": "4.17.9",
    "@types/jest": "26.0.19",
    "@types/js-yaml": "3.12.5",
    "@types/mdast": "3.0.3",
    "@types/unist": "2.0.3",
    "babel-jest": "26.6.3",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "jest": "26.6.3",
    "mock-fs": "4.13.0",
    "nodemon": "2.0.6",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kanej/dmnotes",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dmnotes",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kanej/dmnotes",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/development-server.ts\"",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint ./src --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint src --ext ts"
  },
  "types": "lib/index.d.ts",
  "volta": {
    "extends": "../../package.json"
  }
}
