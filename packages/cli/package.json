{
  "name": "dmnotes",
  "description": "Run a server that hosts your DM notes",
  "version": "0.1.0",
  "author": "John Kane @kanej",
  "bin": {
    "dmnotes": "./bin/run"
  },
  "bugs": "https://github.com/kanej/dmnotes/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "apollo-server-express": "2.19.0",
    "cli-ux": "5.5.1",
    "express": "4.17.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/express": "4.17.9",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "nodemon": "2.0.6",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kanej/dmnotes",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dmnotes",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kanej/dmnotes",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/development-server.ts\"",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint ./src --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,tsx,js,jsx}\""
  },
  "types": "lib/index.d.ts",
  "volta": {
    "extends": "../../package.json"
  }
}
