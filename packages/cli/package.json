{
  "name": "@dungeon-notes/writer",
  "description": "Create D & D 5e adventures in markdown",
  "version": "0.3.3",
  "author": "John Kane @kanej",
  "bin": {
    "dnwriter": "./bin/run"
  },
  "bugs": "https://github.com/kanej/dungeon-notes/issues",
  "dependencies": {
    "@dungeon-notes/engine": "^0.3.2",
    "@dungeon-notes/types": "^0.3.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "apollo-server-express": "2.19.0",
    "chalk": "4.1.0",
    "cli-ux": "5.5.1",
    "cors": "2.8.5",
    "express": "4.17.1",
    "handlebars": "4.7.7",
    "js-yaml": "3.14.1",
    "mdast": "3.0.0",
    "rehype-format": "3.1.0",
    "rehype-stringify": "8.0.0",
    "remark-frontmatter": "3.0.0",
    "remark-parse": "9.0.0",
    "remark-rehype": "8.0.0",
    "remark-stringify": "9.0.1",
    "tslib": "^1",
    "unified": "9.2.1",
    "unist-util-find": "1.0.2",
    "unist-util-is": "4.0.4",
    "unist-util-remove": "2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@dungeon-notes/writer-web": "^0.3.0",
    "@oclif/dev-cli": "1.26.0",
    "@types/express": "4.17.9",
    "@types/jest": "26.0.19",
    "@types/js-yaml": "3.12.5",
    "@types/mdast": "3.0.3",
    "@types/unist": "2.0.3",
    "babel-jest": "26.6.3",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "nodemon": "2.0.6",
    "ts-node": "9.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/www",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kanej/dungeon-notes",
  "keywords": [
    "dungeon-notes",
    "dnd"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dnwriter",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kanej/dungeon-notes",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/development-server.ts\"",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && cpx \"./src/templates/*.handlebars\" ./lib/templates --clean && oclif-dev manifest && oclif-dev readme",
    "test": "jest --passWithNoTests",
    "version": "oclif-dev readme && git add README.md",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint --fix src --ext ts",
    "build": "tsc --build",
    "clean": "rimraf lib *.tsbuildinfo"
  },
  "types": "lib/index.d.ts",
  "volta": {
    "extends": "../../package.json"
  }
}
